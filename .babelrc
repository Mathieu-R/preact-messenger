{
  "sourceMaps": true,
  "presets": [
    ["es2015", {"modules": false}], 
     "react"
  ],
  "plugins": [
    ["transform-es2015-modules-commonjs"],
    ["transform-decorators-legacy"],
    ["transform-react-jsx", {"pragma": "h"}]
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}
